+    width: percentage(math.div(100, 4));
+  }
+}
+
+.code-editor .lang-options {
+  max-width: 480px;
+}
+
+.custom-select-input {
+  border: 1px solid #DDD;
+  border-radius: 4px;
+}
+
+
+// ==================
+// @import "header";
+// ==================
+header .grid {
+  grid-template-columns: auto min-content auto;
+  max-width: 1400px;
+  margin: 0 auto !important;
+  padding: 0 16px;
+}
+
+header {
+  border-bottom: none;
+  box-shadow: 0 0 24px rgb(0 0 0 / 25%);
